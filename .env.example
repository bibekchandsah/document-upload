# GitHub Token for logging to webservicelogs repository
# Generate a token at: https://github.com/settings/tokens
# Required scopes: repo (full control of private repositories)
LOGS_GITHUB_TOKEN=your_github_token_here

# Username for logging repository owner
LOGS_USERNAME=github_username_here

# Parent repository for logs
LOGS_REPOSITORY=webservicelogs

# Folder name in the webservicelogs repository where logs will be stored
LOGS_FOLDER_NAME=merodocument

# CSV file name for storing activity logs
LOGS_FILE_NAME=logs.csv
